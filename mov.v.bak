module mov(
    input [15:0] data_acc,
    input [15:0] data_imm,
    input sel_imm, en,
    output reg [15:0] data_out
);

always @(*)begin
  if(en)begin
    case ({sel_xy,sel_imm})
      2'b00: data_out = data_acc;
      2'b01: data_out = data_imm;
      2'b10: data_out = data_acc;
      2'b11: data_out = data_imm;
      
    endcase

  end
end
endmodule